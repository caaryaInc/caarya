		inset 0px 0px 0px rgb(0, 0, 0);
	box-shadow:
		0px 1px 3px black; 

}

.number {
	font-size: 41px;
	font-weight: normal;
	font-family: eurostile;
}

.regheadmenu {
	display:inline-block;
	vertical-align: middle;
	position: relative;
	height: 100%;
}

.regbox{
	width: 52%;
	margin-top:-2px; 
	margin-left: 9%;
}

.rbox{
	width: 2px; 
	margin-left:0%; 
	margin-right: 1%;
	margin-top: -20px;
	border-left: 2px solid #86ac6a;
}

.connect{
	width: 33%; 
	right: 0px; 
	margin-left: 0%;
	margin-right: 0.5%;
}

/* iPads (portrait) ----------- */
	@media only screen 
	and (min-device-width : 768px) 
	and (max-device-width : 1024px)
	and (orientation : portrait) {
	/* Styles */
		ul#info{
			padding: 0;
			position: relative;
			width: 100%;
			height: 55%;
			margin-top: 58%;
		}

		#registerbox{
			margin-top: 0%;
		}

		#mapholder {
			margin-top: 15%;
			width: 59%;
			height:600px;
			margin-right: 20px;
		}

		#map {
			overflow: hidden;
			height: 600px;  
		}

		ul#info li {
			margin: 6.5% 6.5% 13%;
			font-size: 15px;
		}

		.rbox{
			margin-right: 0.5%;
		}
	}

/* iPads (landscape) ----------- */
	@media only screen 
	and (min-device-width : 768px) 
	and (max-device-width : 1024px)
	and (orientation : landscape) {
	/* Styles */

		#mapholder {
			margin-right: 20px;
		}

		ul#info li {
			font-size: 15px;
		}

		.number {
			font-size: 44px;
			font-weight: normal;
			font-family: eurostile;
		}

		.rbox {
			margin-right: 0.5%;
		}
	}

/*Iphone */

	@media screen and (device-width:320px) and (orientation:landscape) {
		/* iPhone Landscape Styles Here*/
			
		html, body {
			position: relative;
			text-align: center;
			height: 500px;
		}

		#mapholder {
			height:460px;
		}

		#map {
			height: 460px;  
		}
			
		.rbox {
			margin-right: 0%;
		}

		#registerbox {
			margin-top: 0%;
		}
	}

	@media screen and (device-width:320px) and (orientation:portrait) {
		/* iPhone Portrait Styles Here*/
				
		html, body {
			position: relative;
			text-align: center;
			height: 500px;
		}

		#mapholder {
			height:460px;
		}

		#map {
			height: 460px;  
		}

		.rbox {
			margin-right: 0%;
		}

		#registerbox {
			margin-top: 0%;
		}
	}

