html, body {
	position: relative;
	text-align: center;
	height: 83%;
}

#container {
	height: 100%;
}

#pagewrap {
	text-align: left;
	width: 98%;
	height: 100%;
	position: relative;
	margin-left: 0px;	
}

#leftpanel {
	margin-left: 50px;
	width: 33%;
	position:relative;
	height: 98%;
  }

#pagewrap h2 {
	margin-top: 20px;
	font-family: 'Arial Bold';
	color: #333;
	font-size: 20px;
}

ul#info {
	padding: 0;
	position: relative;
	width: 100%;
	height: 72.5%;
	
}

ul#info li {
	list-style: none;
	position: relative;
	padding: 0.5em 3em 0.5em;
	margin: 6.5% 6.5% 7%;
	font-size: 15px;
	color:#000;
	line-height: 1.28;
	letter-spacing: 1px;
	vertical-align: center;
}

ul li#moneyinfo {
	background: url('img/moneyicon.png') no-repeat left center;
}

ul li#tasksinfo {
	background: url('img/tasksicon.png') no-repeat left center;
	margin-left: 20px;
	padding-left: 3.5em;
}

#mapholder {
	position: relative;
	top: -15px;
	width: 57.5%; 
	margin-bottom: 10px;
	height: 98%;
}

#map {
	overflow: hidden;
	height: 98%;  
}

.effect2 {
	position: relative;
}

.effect2:before, .effect2:after {
	z-index: -1;
	position: absolute;
	content: "";
	bottom: 15px;
	width: 40%;
	top: 80%;
	max-width:210px;
  background: #777;
	left: 12px;
  -webkit-box-shadow: 0 15px 10px #777;
  -moz-box-shadow: 0 15px 10px #777;
  box-shadow: 0 15px 10px #777;
  -webkit-transform: rotate(-3deg);
  -moz-transform: rotate(-3deg);
  -o-transform: rotate(-3deg);
  -ms-transform: rotate(-3deg);
  transform: rotate(-3deg);
}
.effect2:after
{
  -webkit-transform: rotate(3deg);
  -moz-transform: rotate(3deg);
  -o-transform: rotate(3deg);
  -ms-transform: rotate(3deg);
  transform: rotate(3deg);
  right: 10px;
  left: auto;
}



#sitedesc li{
	list-style: none;
	display: inline;
	margin-left: 20px;
	padding-left: 1.8em;
	background: url('img/ico-style4.png') no-repeat left center;
}



