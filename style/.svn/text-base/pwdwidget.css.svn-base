.pwdwidgetdiv
{
	width:225px;
	color: #fff;
}

.pwdfield
{
    display:block;
}

.pwdopsdiv
{
	display: block;
	float: left;
	margin-right:6px;	
	color: #fff;
}
.pwdopsdiv a
{
	font-family : Arial, Helvetica, sans-serif;
	font-size : 10px; 
	color: #fff;
	text-decoration:none;
}

.pwdstrengthbar
{
	float:right;
	background:#cccccc;
	height:4px;
	margin:0;
	color: #fff;
}

.pwdstrength
{
	float:right; 
	height:20px;
	width:70px;
	margin-top:3px;

}
.pwdstrengthstr
{
    float:right;
	clear:both;
	height:14px;
	margin-top:0px;
	font-family : Arial, Helvetica, sans-serif;
	font-size : 10px; 

}